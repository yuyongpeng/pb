        }
      }
    },
    "ChainInfo": {
      "type": "object",
      "properties": {
        "chainId": {
          "type": "integer",
          "format": "int64",
          "description": "上链表id"
        },
        "sourceIds": {
          "type": "string",
          "description": "上链表数据来源id 多个逗号分割"
        },
        "sourceType": {
          "type": "integer",
          "format": "int64",
          "description": "上链数据来源：0:da_credential表id;1:collection表id;2:collection_transaction表id,3:collection_checkin表,4:collection_batch表"
        },
        "address": {
          "type": "string",
          "description": "资产持有地址"
        },
        "tokenIds": {
          "type": "string",
          "description": "tokenId 多个逗号分割"
        },
        "txId": {
          "type": "string",
          "description": "上链交易ID"
        },
        "txIdTmp": {
          "type": "string",
          "description": "上链交易临时ID"
        },
        "blockNumber": {
          "type": "integer",
          "format": "int64",
          "description": "上链产生交易在链上的区块号"
        },
        "chainStatus": {
          "type": "integer",
          "format": "int64",
          "description": "上链状态：0-待上链；1-已上链；2-上链中；3-上链失败"
        },
        "txKafka": {
          "type": "string",
          "description": "从kafka 接收到的上链数据"
        },
        "txPostreq": {
          "type": "string",
          "description": "调用bass系统接口传递的数据"
        },
        "txPostres": {
          "type": "string",
          "description": "调用bass系统接口上链响应数据"
        },
        "txNotifyReq": {
          "type": "string",
          "description": "调用bass系统接口的异步通知数据"
        },
        "chainFailMsg": {
          "type": "string",
          "description": "上链错误信息"
        },
        "retryNum": {
          "type": "integer",
          "format": "int64",
          "description": "尝试上链次数"
        },
        "chainStartTime": {
          "type": "integer",
          "format": "int64",
          "description": "开始上链时间"
        },
        "chainEndTime": {
          "type": "integer",
          "format": "int64",
          "description": "上链时间"
        },
        "chainPort": {
          "type": "integer",
          "format": "int64",
          "description": "上链端口"
        },
        "reqTopic": {
          "type": "string",
          "description": "kafaka 上链请求topic"
        },
        "resTopic": {
          "type": "string",
          "description": "kafaka 上链通知topic"
        },
        "createTime": {
          "type": "integer",
          "format": "int64",
          "description": "创建时间"
        },
        "modifyTime": {
          "type": "integer",
          "format": "int64",
          "description": "最后修改时间"
        }
      }
    },
    "ChainListReq": {
      "type": "object",
      "properties": {
        "chainStatus": {
          "type": "string",
          "description": "上链状态：空全部；0-待上链；1-已上链；2-上链中；3-上链失败；"
        },
        "chainTimeStart": {
          "type": "integer",
          "format": "int64",
          "description": "上链时间 (时间戳秒)最小"
        },
        "chainTimeEnd": {
          "type": "integer",
          "format": "int64",
          "description": "上链时间 (时间戳秒)最大 "
        },
        "createTimeStart": {
          "type": "integer",
          "format": "int64",
          "description": "创建时间 (时间戳秒)最小"
        },
        "createTimeEnd": {
          "type": "integer",
          "format": "int64",
          "description": "创建时间 (时间戳秒)最大 "
        },
        "page": {
          "type": "integer",
          "format": "int64",
          "description": "页数 默认1"
        },
        "limit": {
          "type": "integer",
          "format": "int64",
          "description": "条数 默认20"
        }
      }
    },
    "ChainListResp": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int64"
        },
        "msg": {
          "type": "string"
        },
        "data": {
          "$ref": "#/definitions/Data"
        }
      }
    },
    "ChainNotifyReq": {
      "type": "object",
      "properties": {
        "chainId": {
          "type": "integer",
          "format": "int64",
          "title": "上链表id"
        },
        "operatorAddress": {
          "type": "string",
          "title": "操作员账号"
        },
        "contractAddress": {
          "type": "string",
          "title": "链上合约地址"
        },
        "txId": {
          "type": "string",
          "title": "交易id"
        },
        "blockNumber": {
          "type": "integer",
          "format": "int64",
          "title": "上链生成的交易所在区块号"
        },
        "chainStatus": {
          "type": "integer",
          "format": "int64",
          "title": "上链状态：1-成功；3-失败"
        },
        "chainMsg": {
          "type": "string",
          "title": "上链失败原因"
        },
        "opt": {
          "type": "string",
          "title": "透传参数"
        }
      }
    },
    "ChainNotifyTx": {
      "type": "object",
      "properties": {
        "chain": {
          "type": "integer",
          "format": "int32"
        },
        "address": {
          "type": "string"
        },
        "method": {
          "type": "string"
        },
        "args": {
          "type": "string"
        },
        "account": {
          "type": "string"
        },
        "contract": {
          "type": "string"
        },
        "opts": {
          "type": "string"
        }
      }
    },
    "CollectionCredentialClaim": {
      "type": "object",
      "properties": {
        "collectionName": {
          "type": "string",
          "description": "数字藏品名称"
        },
        "claimSignAddress": {
          "type": "string",
          "description": "藏品链上持有人用户userAddress"
        },
        "didCredential": {
          "type": "string",
          "description": "合约持有人身份存证"
        },
        "copyrightCredential": {
          "type": "string",
          "description": "版权声明存证身份存证(多个版权声明英文分号分割';')"
        },
        "authorizationCredential": {
          "type": "string",
          "description": "授权存证"
        },
        "collectionHash": {
          "type": "string",
          "description": "数字藏品名称、持有人人用户ID和时间按顺序拼成一个字符串所做的Keccak256哈希"
        },
        "tokenId": {
          "type": "string",
          "description": "藏品tokenId"
        },
        "from": {
          "type": "string",
          "description": "铸造人userAddress"
        },
        "declareTime": {
          "type": "string",
          "description": "声明时间(2022-09-01 10:00:00)"
        },
        "extension": {
          "type": "string",
          "description": "扩展信息(应用可自行定义扩展属性)"
        }
      },
      "required": [
        "collectionName",
        "claimSignAddress",
        "didCredential",
        "copyrightCredential",
        "authorizationCredential",
        "from",
        "declareTime"
      ]
    },
