    "BatchPostInfo": {
      "type": "object",
      "properties": {
        "sourceIds": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          },
          "description": "来源id 格式[1,2]"
        },
        "sourceType": {
          "type": "integer",
          "format": "int64",
          "description": "1:collection表,2:collection_transaction表,3:collection_checkin表,4:collection_batch表,5:collection_transaction_batch表"
        },
        "address": {
          "type": "string",
          "description": "合约地址"
        },
        "chain": {
          "type": "integer",
          "format": "int64",
          "description": "端口 bsn:5555 hashIi:6666"
        }
      }
    },
    "BatchPostMintReq": {
      "type": "object",
      "properties": {
        "batchPost": {
          "$ref": "#/definitions/BatchPostInfo",
          "description": "mint提交基本信息"
        },
        "args": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/TxMintBatchArgs"
          },
          "description": "mint提交参数  最多100个"
        }
      },
      "required": [
        "batchPost",
        "args"
      ]
    },
    "BatchPostTransferReq": {
      "type": "object",
      "properties": {
        "batchPost": {
          "$ref": "#/definitions/BatchPostInfo",
          "description": "transfer提交基本信息"
        },
        "args": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/TxTransferBatchArgs"
          },
          "description": "transfer提交参数 最多100个"
        }
      },
      "required": [
        "batchPost",
        "args"
      ]
    },
    "BatchPostUseReq": {
      "type": "object",
      "properties": {
        "batchPost": {
          "$ref": "#/definitions/BatchPostInfo",
          "description": "use提交基本信息"
        },
        "args": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "use提交参数 最多100个"
        }
      },
      "required": [
        "batchPost",
        "args"
      ]
    },
    "ChainAgainReq": {
      "type": "object",
