      "type": "object",
      "properties": {
        "context": {
          "type": "string",
          "description": "声明地址"
        },
        "id": {
          "type": "string",
          "description": "声明uuid"
        },
        "issuer": {
          "type": "string",
          "description": "签名address"
        },
        "proof": {
          "type": "string",
          "description": "权益声明签名"
        },
        "claim": {
          "$ref": "#/definitions/CollectionCredentialClaim",
          "description": "权益声明"
        }
      },
      "required": [
        "context",
        "id",
        "issuer",
        "proof",
        "claim"
      ]
    },
