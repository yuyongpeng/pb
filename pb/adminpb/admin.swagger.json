  "definitions": {
    "Account": {
      "type": "object",
      "properties": {
        "accountId": {
          "type": "integer",
          "format": "int64",
          "description": "账号ID"
        },
        "accountName": {
          "type": "string",
          "description": "登录用户名"
        },
        "password": {
          "type": "string",
          "default": "66668888",
          "description": "登录密码"
        },
        "roleId": {
          "type": "integer",
          "format": "int64",
          "description": "角色ID"
        },
        "roleNameCn": {
          "type": "string",
          "description": "中文名"
        },
        "roleNameEn": {
          "type": "string",
          "description": "英文名"
        },
        "groupId": {
          "type": "integer",
          "format": "int64",
          "description": "分组ID"
        },
        "groupName": {
          "type": "string",
          "description": "分组名称"
        },
        "realName": {
          "type": "string",
          "description": "真实姓名"
        },
        "avatar": {
          "type": "string",
          "description": "头像链接"
        },
        "mobile": {
          "type": "string",
          "description": "手机号码"
        },
        "email": {
          "type": "string",
          "description": "邮箱"
        },
        "remarks": {
          "type": "string",
          "description": "简介"
        },
        "bindIp": {
          "type": "string",
          "description": "绑定登录IP"
        },
        "bindMac": {
          "type": "string",
          "description": "绑定登录MAC地址"
        },
        "operatorId": {
          "type": "integer",
          "format": "int64",
          "description": "最后修改人ID"
        },
        "operatorName": {
          "type": "string",
          "description": "真实姓名"
        },
        "lastLoginTime": {
          "type": "integer",
          "format": "int64",
          "description": "创建时间"
        },
        "lastLoginIp": {
          "type": "string",
          "description": "创建时间"
        },
        "lastLoginMac": {
          "type": "string",
          "description": "创建时间"
        },
        "initState": {
          "type": "integer",
          "format": "int64",
          "default": "2",
          "description": "初始状态：1-系统初始用户;2-非系统初始用户"
        },
        "isSystemAdmin": {
          "type": "integer",
          "format": "int64",
          "default": "2",
          "description": "是否为系统管理员：1-是;2-否"
        },
        "createTime": {
          "type": "integer",
          "format": "int64",
          "description": "创建时间"
        },
        "modifyTime": {
          "type": "integer",
          "format": "int64",
          "description": "最后修改时间"
        },
        "status": {
          "type": "integer",
          "format": "int64",
          "default": "1",
          "description": "状态：0-删除;1-有效;2-锁定"
        }
      },
      "title": "账号相关数据结构",
      "required": [
        "accountName",
        "roleId",
        "roleNameCn"
      ]
    },
    "AdminPresentByAccountReq": {
      "type": "object",
      "properties": {
        "account": {
          "type": "string",
          "description": "账号"
        },
        "amount": {
          "type": "integer",
          "format": "int64",
          "description": "赠送能量值"
        },
        "remark": {
          "type": "string",
          "description": "备注",
          "title": "ID"
        }
      }
    },
    "AdminPresentByAccountResp": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "无符型整数",
          "description": "错误代码,0表示业务成功,否则表示错误",
          "title": "错误代码,0表示业务成功,否则表示错误"
        },
        "msg": {
          "type": "string",
          "description": "错误信息",
          "title": "错误信息"
        }
      }
    },
    "AuthorisePrivilegesToRoleReq": {
      "type": "object",
      "properties": {
        "roleId": {
          "type": "integer",
          "format": "int64",
          "description": "角色ID"
        },
        "privilegeId": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          },
          "description": "权限ID列表"
        }
      },
      "title": "保存角色权限请求",
      "required": [
        "roleId",
        "privilegeId"
      ]
    },
    "BaseResp": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int64",
          "description": "错误码：0-成功；其它都为失败"
        },
        "msg": {
          "type": "string",
          "description": "错误描述"
        },
        "data": {}
      },
      "title": "返回的基础结构类型"
    },
    "GetAccountAddressGasInfo": {
      "type": "object",
      "properties": {
        "accountAddress": {
          "type": "string",
          "description": "账号address"
